{
  "schemaVersion": 1,
  "id": "jilibs_woodset",
  "name": "Ji Libs Wood Set",
  "description": "Contains the essential API for registring new wood type and it's family",
  "version": "${version}",
  "environment": "*",
  "accessWidener" : "jilibs_woodset.accesswidener",
  "mixins": [
    "jilibs_woodset.mixins.json",
    {
      "config": "jilibs_woodset.client.mixins.json",
      "environment": "client"
    }
  ],
  "license": "MIT",
  "icon": "assets/jilibs_woodset/icon.png",
  "contact": {
    "homepage": "https://www.youtube.com/@TheMentorCodeLab",
    "issues": "https://github.com/drkhodakarami/JiLibs/issues",
    "sources": "https://github.com/drkhodakarami/JiLibs"
  },
  "authors": [
    "The Mentor"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "~${game_version}",
    "java": ">=${java_version}",
    "fabric-api": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/pmM4emCbuH"
      },
      "badges": ["library"],
      "parent": {
        "id": "jilibs",
        "name": "Ji Libs",
        "description": "Contains API for registering new wood type and family",
        "icon": "assets/jilibs/icon.png",
        "badges": [ "library" ]
      },
      "update_checker": true
    }
  }
}